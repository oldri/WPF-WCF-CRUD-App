<Window
    x:Class="AS2_991666875_S.Views.MainView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:components="clr-namespace:AS2_991666875_S.UserControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AS2_991666875_S.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="AppWindow"
    Title="Fitbit App"
    Width="960"
    Height="800"
    MinWidth="960"
    MinHeight="800"
    Icon="/Assets/Images/northwind.png"
    MouseDown="AppWindow_MouseDown"
    Style="{StaticResource WindowStyle}"
    WindowStartupLocation="CenterScreen"
    mc:Ignorable="d">
    <components:ShellMainUC x:Name="ucShellMain"/>
</Window>
